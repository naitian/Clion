#!/Users/naitian/Documents/School/CS/Projects/clion2/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'clion','console_scripts','clion'
__requires__ = 'clion'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('clion', 'console_scripts', 'clion')()
    )
